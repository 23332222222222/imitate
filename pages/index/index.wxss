.slides {
  height: 380rpx;
}
.slides navigator {
  height: 100%;
}
.slides navigator image {
  width: 100%;
  height: 100%;
}
.categories {
  display: flex;
  flex-wrap: wrap;
  border-left: 1px solid #ccc;
  border-top: 1px solid #ccc;
  background: #fff;
}
.categories navigator {
  width: 33.33333%;
  height: 250rpx;
  text-align: center;
  box-sizing: border-box;
  border-right: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
}
.categories navigator image {
  width: 30%;
  height: 30%;
  margin: 25% auto 20rpx;
  display: block;
}
.categories navigator text {
  font-size: 24rpx;
}
.links {
  display: flex;
  justify-content: space-between;
  margin: 10rpx 0;
  background: #fff;
  padding: 30rpx;
}
.links navigator image {
  width: 330rpx;
  height: 185rpx;
}
